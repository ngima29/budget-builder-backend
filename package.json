{
  "name": "mumme-backend",
  "version": "1.0.0",
  "description": "mumme payment backend api",
  "main": "server.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "build:watch": "tsc --watch",
    "start:watch": "nodemon dist/server.js",
    "start:dev": "nodemon ./src/server.ts",
    "clean": "rm -rf ./dist",
    "migrate": "ts-node node_modules/.bin/sequelize db:migrate --config=src/config/database.js"
  },
  "keywords": [
    "mumme",
    "api",
    "backend"
  ],
  "author": "Kamal Aryal",
  "license": "ISC",
  "dependencies": {
    "@joi/date": "^2.1.0",
    "axios": "^1.2.1",
    "boom": "^7.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "sequelize": "^6.26.0",
    "sequelize-cli": "^6.5.2",
    "slug": "^8.2.2",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.6.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/register": "^7.18.9",
    "@types/boom": "^7.3.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.11",
    "@types/sequelize": "^4.28.14",
    "@types/slug": "^5.0.3",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
